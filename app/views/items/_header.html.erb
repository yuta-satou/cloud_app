<div class='header-main'>
  <div class='header'>
    <div class='heading'>
      <%# <h1 class='title'>タイトル</h1> %>
      <%= image_tag "baseball 7.jpg", size: '80x70' %>
    </div>
    <div class='navbar'>
      <ul class='nav-lists'>
        <li><a class="class" href="#">クラウドファンディングをはじめる</a></li>
        <li>さがす</li>
        <% if user_signed_in? %>
          <li><%= "#{current_user.nickname}" %></li>
          <li><%= link_to 'ログアウト', destroy_user_session_path, method: :delete, class: 'class' %></li>
        <% else %>
          <li><%= link_to 'ログイン',  new_user_session_path, class: 'class' %></li>
          <li><%= link_to '新規登録', new_user_registration_path, class: 'class' %></li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
